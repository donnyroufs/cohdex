{
  "name": "kondah-app",
  "version": "1.0.0",
  "main": "bootstrap.ts",
  "author": "kondah",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --no-notify src/bootstrap.ts",
    "migrate": "prisma migrate dev",
    "test": "jest --collect-coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/connect-redis": "^0.0.16",
    "@types/cors": "^2.8.10",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/passport": "^1.0.6",
    "@types/passport-steam": "^1.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": ">=11",
    "prettier": "^2.2.1",
    "prisma": "^2.22.1",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
